{
    "name": "Element Remover",
    "version": "1.0",
    "description": "Removes elements from right-click menu",
    "manifest_version": 2,
    "permissions": ["contextMenus"],
    "background": {
        "scripts": ["back.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "icons": {
        "16": "ic16.png",
        "32": "ic32.png",
        "48": "ic48.png",
        "128": "ic128.png"
    }
}